function GodModeON()
local char = game:GetService("Players").LocalPlayer.Character
char.Head.CanTouch = false
char.Torso.CanTouch = false
char["Left Arm"].CanTouch = false
char["Right Arm"].CanTouch = false
char["Left Leg"].CanTouch = false
char["Right Leg"].CanTouch = false
char.HumanoidRootPart.CanTouch = false

char.Head.CanCollide = false
char.Torso.CanCollide = false
char["Left Arm"].CanCollide = false
char["Right Arm"].CanCollide = false
char["Left Leg"].CanCollide = false
char["Right Leg"].CanCollide = false
char.HumanoidRootPart.CanCollide = false

char.Head.CanQuery = false
char.Torso.CanQuery = false
char["Left Arm"].CanQuery = false
char["Right Arm"].CanQuery = false
char["Left Leg"].CanQuery = false
char["Right Leg"].CanQuery = false
char.HumanoidRootPart.CanQuery = false

char.HeadPart.Middle.CanTouch = false
char.Arm2.Middle.CanTouch = false
char.Arm1.Middle.CanTouch = false
char.Chest.Middle.CanTouch = false
char.Chest.Sheathe.CanTouch = false
char.Chest.SheatheEnd.CanTouch = false
char.Leg1.Middle.CanTouch = false
char.Leg2.Middle.CanTouch = false
char.Tors.a.Head.CanTouch = false
char.Tors.a.Torso.CanTouch = false
char.Tors.a["Left Arm"].CanTouch = false
char.Tors.a["Right Arm"].CanTouch = false
char.Tors.a["Left Leg"].CanTouch = false
char.Tors.a["Right Leg"].CanTouch = false
char.Tors.a.RootPart.CanTouch = false

char.HeadPart.Middle.CanCollide = false
char.Arm2.Middle.CanCollide = false
char.Arm1.Middle.CanCollide = false
char.Chest.Middle.CanCollide = false
char.Chest.Sheathe.CanCollide = false
char.Chest.SheatheEnd.CanCollide = false
char.Leg1.Middle.CanCollide = false
char.Leg2.Middle.CanCollide = false
char.Tors.a.Head.CanCollide = false
char.Tors.a.Torso.CanCollide = false
char.Tors.a["Left Arm"].CanCollide = false
char.Tors.a["Right Arm"].CanCollide = false
char.Tors.a["Left Leg"].CanCollide = false
char.Tors.a["Right Leg"].CanCollide = false
char.Tors.a.RootPart.CanCollide = false

char.HeadPart.Middle.CanQuery = false
char.Arm2.Middle.CanQuery = false
char.Arm1.Middle.CanQuery = false
char.Chest.Middle.CanQuery = false
char.Chest.Sheathe.CanQuery = false
char.Chest.SheatheEnd.CanQuery = false
char.Leg1.Middle.CanQuery = false
char.Leg2.Middle.CanQuery = false
char.Tors.a.Head.CanQuery = false
char.Tors.a.Torso.CanQuery = false
char.Tors.a["Left Arm"].CanQuery = false
char.Tors.a["Right Arm"].CanQuery = false
char.Tors.a["Left Leg"].CanQuery = false
char.Tors.a["Right Leg"].CanQuery = false
char.Tors.a.RootPart.CanQuery = false
end

function GodModeOFF()
local char = game:GetService("Players").LocalPlayer.Character
