char.Torso.CanTouch = true
char["Left Arm"].CanTouch = true
char["Right Arm"].CanTouch = true
char["Left Leg"].CanTouch = true
char["Right Leg"].CanTouch = true
char.HumanoidRootPart.CanTouch = true

char.Head.CanCollide = true
